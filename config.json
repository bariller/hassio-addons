{
  "name": "RF Receiver",
  "version": "1",
  "slug": "rf_receiver",
  "description": "433Mhz RF Receiver",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "i386"
    ],
    "boot": "auto",
    "devices": ["/dev/mem:/dev/mem:rw"],
    "privileged": ["SYS_RAWIO"],
    "gpio": "true",
    "apparmor": "false",
    "ports": {"5055/tcp": null},
    "options": {
  "code": "/share/rpi-rf_receive.py",
  "requirements": [
    "rpi-rf-gpiod"
  ],
  "clean": true
},
    "schema": {"code": "str",
	       "requirements": ["str"],
	       "clean": "bool?"},
    "map": ["share"]
} 
